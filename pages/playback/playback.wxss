/**index.wxss**/
.imageBox{
  width: 100%;
  height: 35%;
}
.selectSpeedPicker{
  position: absolute;
  top:20px;
  padding-left: 5px;
  padding-right: 5px;
  align-items: right;
  border-radius:20%;
  font-size: 15px;
  color: silver;
  border: 0.5px solid silver;
  right: 20px;
}
.image-front{
  float:left;
  width: 50%;
  height: 100%;
}
.image-after{
  float:right;
  width: 50%;
  height: 100%;
}
.progress{
  /* padding-top: 5px; */
  padding-bottom: 6px;
  width: 100%;
  height:5%;
}

.progress-button{
  padding-top: 10px;
  padding-left: 5px;
  width: 5%;
  float: left;
  height: 62%;
}
.progress-slider{
  width: 60%;
  float: left;
}
.progress-text{
  padding-top: 9px;
  padding-right: 10px;
  width: 20%;
  float: right;
  font-size: 14px;
  text-align: right;
  height: 100%;
  color: rgb(149, 149, 149);

  
}
.map{
  padding-top:5px;
  /* padding-bottom: 60px; */
  text-align: center;
  align-items: center;
  display: flex;
  width: 100%;
  height: 60%;
}
